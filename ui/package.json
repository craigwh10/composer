{
  "author": {
    "name": "Craig White"
  },
  "bundleDependencies": false,
  "deprecated": false,
  "description": "GUI for dependency runner",
  "files": [
    "template"
  ],
  "keywords": [
    "dependency",
    "runner",
    "gui"
  ],
  "private": false,
  "name": "dr-gui",
  "version": "0.1.0",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.50",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.2.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "electron-is-dev": "^1.2.0",
    "electronmon": "^1.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "3.4.1",
    "remote": "^0.2.6",
    "sass": "^1.26.10",
    "typescript": "^3.7.5",
    "wait-on": "^5.1.0"
  },
  "main": "public/electron-main.js",
  "scripts": {
    "start": "concurrently \"npm run react-serve\" \"wait-on http://localhost:3000 && npm run electron-start\"",
    "build": "npm run react-build && npm run electron-build",
    "release": "npm run react-build && npm run electron-release",
    "electron-start": "electronmon .",
    "electron-build": "electron-builder",
    "electron-release": "electron-builder --publish=always",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-serve": "BROWSER=none && npm run react-start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "./",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.1",
    "dotenv": "^10.0.0"
  }
}
